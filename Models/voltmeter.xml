<?xml version="1.0"?>

<!-- Voltmeter -->

<PropertyList>

 <path>voltmeter.ac</path>

<animation>
  <type>rotate</type>
  <object-name>voltmeter-Needle1</object-name>
  <property>systems/electrical/outputs/annunciators</property>
  <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>		<!-- 0v  -->
   <entry><ind>15</ind><dep>110</dep></entry>		<!-- 15v -->
   <entry><ind>20</ind><dep>142</dep></entry>		<!-- 20v -->
   <entry><ind>25</ind><dep>170</dep></entry>		<!-- 25v -->
   <entry><ind>28</ind><dep>189</dep></entry>		<!-- 29v -->
   <entry><ind>30</ind><dep>193</dep></entry>	 	<!-- 30v -->
   <entry><ind>35</ind><dep>218</dep></entry> 		<!-- 35v -->
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <!-- *** LoD *** -->
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5</max-m>
 </animation>


</PropertyList>
